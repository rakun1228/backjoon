import java.util.Scanner;

class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        sc.nextLine();
        String ans = sc.nextLine();
        int sum = 0;
        
        for(int i=0;i<n;i++){
            int num = ans.charAt(i)-48;
            //System.out.println(num);
            sum+=num;
        }
        System.out.println(sum);
    }
}
