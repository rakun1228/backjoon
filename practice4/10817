import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = sc.nextInt();
		
		if(c>b) {
			if(a>c) System.out.println(c);
			else {
				if(a>b) System.out.println(a);
				else System.out.println(b);
			}
		}
		else {
			if(a<c) System.out.println(c);
			else {
				if(a<b) System.out.println(a);
				else System.out.println(b);
			}
		}
	}
}
